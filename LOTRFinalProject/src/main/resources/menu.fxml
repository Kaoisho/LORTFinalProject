<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kaoishoworks.lotrfinalproject.Controllers.MenuController">
   <children>
      <ImageView fitHeight="785.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/background_menu.JPG" />
         </image>
      </ImageView>
      <Button fx:id="mainBattleButton" layoutX="509.0" layoutY="408.0" mnemonicParsing="false" onAction="#launchMainBattle" prefHeight="75.0" prefWidth="273.0" stylesheets="@style/button.css" text="Batalla Predeterminada">
         <font>
            <Font name="Candara Bold" size="24.0" />
         </font></Button>
      <Button fx:id="customBattleButton" layoutX="509.0" layoutY="531.0" mnemonicParsing="false" onAction="#launchCustomBattle" prefHeight="75.0" prefWidth="273.0" stylesheets="@style/button.css" text="Batalla Personalizada">
         <font>
            <Font name="Candara Bold" size="26.0" />
         </font></Button>
   </children>
</AnchorPane>
